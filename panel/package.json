    "dotenv": "16.3.1",
    "fastq": "1.15.0",
    "fuzzysort": "2.0.4",
    "jotai": "2.4.2",
    "next": "14.0.4",
    "next-intl": "3.5.4",
    "node-fetch": "2.6.6",
    "node-html-parser": "6.1.10",
    "nodemailer": "6.9.5",
    "prisma": "5.7.0",
    "qrcode": "1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.0",
