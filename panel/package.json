{
  "name": "@set/panel",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "SKIP_ENV_VALIDATION=true next build",
    "export": "next build && next export",
    "generate-css-types": "tailwindcss-classnames -i tailwind.config.ts -o tailwind-types.ts",
    "generate-prisma": "prisma generate --schema=./server/prisma/schema.prisma",
    "dev:ws": "tsx --tsconfig tsconfig.server.json server/wsDevServer.ts",
    "dev:ws:debug": "export DEBUG=prisma* && tsx --tsconfig tsconfig.server.json server/wsDevServer.ts",
    "dev:next": "next dev",
    "dev:next:debug": "export DEBUG=prisma* && next dev",
    "dev:start": "bun dev:ws & bun dev:next",
    "prod:ws": "tsx --tsconfig tsconfig.server.json server/wsProdServer.ts",
    "prod:next": "next start",
    "prod:start": "bun prod:ws & bun prod:next",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "seed": "bun server/prisma/seed.ts",
    "schema": "server/prisma/schema.prisma"
  },
  "dependencies": {
    "@faker-js/faker": "8.0.2",
    "@headlessui-float/react": "0.11.4",
